  "author": "Consensys Software Inc.",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "prettier": "prettier --check \"**/*.{json,md,yml,js,ts}\"",
