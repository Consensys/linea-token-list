  "devDependencies": {
    "@types/deep-diff": "^1.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.33",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": "22.22.0",
    "npm": ">=10.0.0"
  },
  "_moduleAliases": {
    "src": "scripts/dist/src"
